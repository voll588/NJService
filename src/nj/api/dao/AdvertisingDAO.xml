<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="nj.api.dao.AdvertisingDAO">

	<select id="selectAdvertising" resultType="java.util.Map">
		select active_id activeId,pic_url picUrl
		from activity_advertising where state = 1
	</select>
	
</mapper>